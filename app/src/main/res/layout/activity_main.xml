<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.omerrom.portabelo.MainActivity"
    android:background="@drawable/screenbgnomenu">


    <ImageButton
        android:drawable="@drawable/stringstrdecayover10"
        android:layout_height="fill_parent"
        android:layout_width="60dp"
        android:contentDescription="@string/string_description"
        android:src="@drawable/stringstrdecayover10"
        android:id="@+id/imageButton"
        android:background="#00917070"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="10dp"
        android:backgroundTint="#00ffffff" />

    <ImageButton
        android:drawable="@drawable/stringstrdecayover10"
        android:layout_height="fill_parent"
        android:layout_width="60dp"
        android:contentDescription="@string/string_description"
        android:src="@drawable/stringstrdecayover10"
        android:background="#00ffffff"
        android:id="@+id/imageButton2"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/imageButton"
        android:layout_toEndOf="@+id/imageButton" />

    <ImageButton
        android:drawable="@drawable/stringstrdecayover10"
        android:layout_height="fill_parent"
        android:layout_width="60dp"
        android:contentDescription="@string/string_description"
        android:src="@drawable/stringstrdecayover10"
        android:background="#00ffffff"
        android:id="@+id/imageButton3"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/imageButton2"
        android:layout_toEndOf="@+id/imageButton2" />

    <ImageButton
        android:drawable="@drawable/stringstrdecayover10"
        android:layout_height="fill_parent"
        android:layout_width="60dp"
        android:contentDescription="@string/string_description"
        android:src="@drawable/stringstrdecayover10"
        android:background="#00ffffff"
        android:id="@+id/imageButton4"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/imageButton3"
        android:layout_toEndOf="@+id/imageButton3" />

    <ImageButton
        android:drawable="@drawable/stringstrdecayover10"
        android:layout_height="fill_parent"
        android:layout_width="60dp"
        android:contentDescription="@string/string_description"
        android:src="@drawable/stringstrdecayover10"
        android:background="#00ffffff"
        android:id="@+id/imageButton5"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/imageButton4"
        android:layout_toEndOf="@+id/imageButton4" />

    <ImageButton
        android:drawable="@drawable/stringstrdecayover10"
        android:layout_height="fill_parent"
        android:layout_width="60dp"
        android:contentDescription="@string/string_description"
        android:src="@drawable/stringstrdecayover10"
        android:background="#00ffffff"
        android:id="@+id/imageButton6"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/imageButton5"
        android:layout_toEndOf="@+id/imageButton5" />
</RelativeLayout>